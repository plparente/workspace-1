package variaveis;

import javax.swing.JOptionPane;

public class AreaTrianguloCirculo {

	public static void main(String[] args) {
		int base = Integer.parseInt(JOptionPane.showInputDialog("Digite a quantidade:"));
		int altura = Integer.parseInt(JOptionPane.showInputDialog("Digite a quantidade:"));
		
		System.out.println("Area do triangulo: " + base * altura);
	}

}
